tableFor "users", (t) ->
  t.head ->
    t.row ->
      
      t.cell "email", sort: true
      
      t.cell()
      t.cell()
      t.cell()
  t.body ->
    for user in @users
      t.row ->
        t.cell user.get("email")
        
        t.cell linkTo 'Show', user
        t.cell linkTo 'Edit', editUserPath(user)
        t.cell linkTo 'Destroy', user, method: "delete"
        
  linkTo 'New User', newUserPath()
